{
    "format_version": "1.8.0",
    "render_controllers": {
        "controller.render.persona_animated_face.first_person": {
            "rebuild_animation_matrices": true,
            "geometry": "Geometry.animated_face",
            "materials": [
                {
                    "*": "Material.animated"
                }
            ],
            "textures": [
                "Texture.animated_face"
            ],
            "part_visibility": [
                {
                    "*": false
                },
                {
                    "rightArm": "q.get_equipped_item_name(0, 1) == '' || q.get_equipped_item_name(0, 1) == 'map'"
                },
                {
                    "rightSleeve": "q.get_equipped_item_name(0, 1) == '' || q.get_equipped_item_name(0, 1) == 'map'"
                },
                {
                    "leftArm": "(q.get_equipped_item_name(0, 1) == 'map' && q.get_equipped_item_name('off_hand') != 'shield') || (q.get_equipped_item_name('off_hand') == 'map' && !q.item_is_charged) || (!q.item_is_charged && (v.item_use_normalized > 0 && v.item_use_normalized < 1.0))"
                },
                {
                    "leftSleeve": "(q.get_equipped_item_name(0, 1) == 'map' && q.get_equipped_item_name('off_hand') != 'shield') || (q.get_equipped_item_name('off_hand') == 'map' && !q.item_is_charged) || (!q.item_is_charged && (v.item_use_normalized > 0 && v.item_use_normalized < 1.0))"
                }
            ],
            "uv_anim": {
                "offset": [
                    0,
                    "v.use_blinking_animation ? v.is_blinking * 0.5 : Math.mod(Math.floor(q.life_time * 7),v.animation_frames_face)/v.animation_frames_face"
                ],
                "scale": [
                    1,
                    1
                ]
            },
            "overlay_color": {
                "r": 1,
                "g": 0,
                "b": 0,
                "a": "!q.is_in_ui && q.has_property('hgncb:visual.hurt') && q.property('hgncb:visual.hurt') > 0 ? 0.4 : 0"
            }
        },
